#include <windows.h>

IDI_ICON1               ICON    "meteoio/icons/meteoio.ico"

1 VERSIONINFO
FILEVERSION @VERSION_MAJOR@,@VERSION_MINOR@,@VERSION_PATCH@,0
PRODUCTVERSION @VERSION_MAJOR@,@VERSION_MINOR@,@VERSION_PATCH@,0
FILEFLAGS 0x0L
FILEFLAGSMASK 0x3fL
FILEOS VOS__WINDOWS32
FILETYPE VFT_APP
FILESUBTYPE 0x0L
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904B0"
        BEGIN
            VALUE "CompanyName", "@CPACK_PACKAGE_VENDOR@"
            VALUE "FileDescription", "@CPACK_PACKAGE_DESCRIPTION_SUMMARY@"
            VALUE "FileVersion", "@VERSION_MAJOR@.@VERSION_MINOR@.@VERSION_PATCH@"
            VALUE "LegalCopyright", "Copyright (C) 2008 - @CURR_YEAR@ WSL/SLF"
            VALUE "InternalName", "@CPACK_PACKAGE_NAME@"
            VALUE "OriginalFilename", "meteoio_timeseries.exe"
            VALUE "ProductName", "@CPACK_PACKAGE_NAME@"
            VALUE "ProductVersion", "@VERSION_MAJOR@.@VERSION_MINOR@.@VERSION_PATCH@"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x0409, 1200
    END
END

